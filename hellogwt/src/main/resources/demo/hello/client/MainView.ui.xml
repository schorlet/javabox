<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <g:SplitLayoutPanel ui:field="splitLayoutPanel">

    <g:north size="25">
      <g:FlowPanel styleName="northPanel">
        <g:TabBar ui:field="tabBar" height="22px" />
      </g:FlowPanel>
    </g:north>

    <g:west size="100">
      <g:LayoutPanel styleName="westPanel">
        <g:layer>
          <g:SimplePanel ui:field="westPanel" />
        </g:layer>
      </g:LayoutPanel>
    </g:west>

    <g:center size="300">
      <g:LayoutPanel styleName="centerPanel">
        <g:layer>
          <g:SimplePanel ui:field="centerPanel" />
        </g:layer>
      </g:LayoutPanel>
    </g:center>

    <g:east size="150">
      <g:LayoutPanel styleName="eastPanel">
        <g:layer>
          <g:SimplePanel ui:field="eastPanel" />
        </g:layer>
      </g:LayoutPanel>
    </g:east>

    <g:south size="125">
      <g:LayoutPanel styleName="southPanel">
        <g:layer>
          <g:SimplePanel ui:field="southPanel" />
        </g:layer>
      </g:LayoutPanel>
    </g:south>

  </g:SplitLayoutPanel>

</ui:UiBinder>
